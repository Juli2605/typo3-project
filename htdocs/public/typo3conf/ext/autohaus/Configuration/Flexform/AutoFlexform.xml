<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Auto</sheetTitle>

                </TCEforms>
                <type>array</type>
                <el>
                    <!-- Add settings here ... -->

                    <!-- Example setting: input field with name settings.includeCategories -->
                    <settings.includeCategories>
                        <TCEforms>
                            <label>Auto</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">
                                            Auto
                                        </numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.includeCategories>
                    <settings.orderBy>
                        <TCEforms>
                            <label>Auto</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">
                                            Auto
                                        </numIndex>
                                        <numIndex index="1">crdate</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">
                                            Auto
                                        </numIndex>
                                        <numIndex index="1">title</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.orderBy>

                    <settings.inputparent>
                        <TCEforms>
                            <label>Choose Parent Id</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.inputparent>

                    <settings.maxSeiten>
                        <TCEforms>
                            <label>Maxseiten</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.maxSeiten>


                    <!-- end of settings -->

                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>